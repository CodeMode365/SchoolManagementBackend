{
  "name": "schoolbakend",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist",
    "start": "NODE_ENV=production bun src/index.ts",
    "test": "bun test",
    "lint": "bunx eslint ./src/* --fix",
    "db:seed": "bun db:clean && bun scripts/seed/seedAll.ts",
    "db:clean": "bun scripts/extra/cleanDb.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.4",
    "@types/morgan": "^1.9.9",
    "eslint": "9.x",
    "globals": "^15.3.0",
    "typescript-eslint": "^7.10.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "http-status-code": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^8.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.13.0"
  }
}
